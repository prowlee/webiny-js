{
  "name": "cms-only",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@webiny/app-template-admin": "^4.0.0-beta.0",
    "@webiny/app-headless-cms": "^4.0.0-beta.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "@webiny/cli": "^4.0.0-beta.0",
    "@webiny/project-utils": "^4.0.0-beta.0"
  },
  "scripts": {
    "start": "env-cmd -r .env.json -e default,local webiny run start",
    "build:dev": "env-cmd -r .env.json -e default,dev webiny run build",
    "build:prod": "env-cmd -r .env.json -e default,prod webiny run build"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
