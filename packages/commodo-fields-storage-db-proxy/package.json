{
	"name": "@webiny/commodo-fields-storage-db-proxy",
	"version": "4.4.0",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/webiny/commodo.git"
	},
	"contributors": [
		"Adrian Smijulj <adrian@webiny.com>"
	],
	"license": "MIT",
	"dependencies": {
		"@commodo/fields": "^1.1.0",
		"bson": "^4.0.2"
	},
	"publishConfig": {
		"access": "public",
		"directory": "dist"
	},
	"keywords": [
		"commodo",
		"composeable",
		"models",
		"storage",
		"mongodb"
	],
	"scripts": {
		"build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\"",
