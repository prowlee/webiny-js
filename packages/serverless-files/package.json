{
  "name": "@webiny/serverless-files",
  "version": "4.4.0",
  "main": "serverless.js",
  "license": "MIT",
  "dependencies": {
    "@webiny/handler": "^4.4.0",
    "@webiny/project-utils": "^4.4.0",
    "aws-sdk": "^2.539.0",
    "object-hash": "^1.3.1",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@types/sharp": "^0.25.0"
  },
  "scripts": {
    "build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts\"",
    "watch": "babel src -d dist --source-maps --copy-files --extensions \".ts\" --watch",
    "postbuild": "cp package.json LICENSE README.md dist/ && tsc -p tsconfig.build.json",
    "deploy": "cd test && sls --debug"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "adio": {
    "ignore": {
      "src": [
        "sharp"
      ]
    }
  },
  "gitHead": "71cab7d5607b2e32a63034e324a663770507b32b"
}
